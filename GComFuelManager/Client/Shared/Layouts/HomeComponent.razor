@page "/"
@page "/inicio"

<AuthorizeView>
    <Authorized>
        <div class="card">
            <div class="card-header col-12">
                <h5 align="center">Bienvenido @context.User.Identity?.Name</h5>
            </div>
        </div>
        <div class="col-12">
            <img class="mx-auto d-block w-50" src="https://i.postimg.cc/Z57T1QGJ/Triptico-Energas-EDITABLE-03.png" alt="ENERGAS DE MEXICO, S.A. DE C.V.">
        </div>
    </Authorized>
